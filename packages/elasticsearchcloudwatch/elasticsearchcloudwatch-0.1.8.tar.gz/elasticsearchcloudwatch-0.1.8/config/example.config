[General]
smtp_server=mailhost.somewhere.com
alert_email=someone@somewhere.com
from_address=someone@somewhere.com

[Elasticsearch]
es_protocol=http
es_url=localhost
es_port=9200
es_suffix=elasticsearch
es_username=
es_password=

[Proxy]
proxy_url=
proxy_port=
proxy_username=
proxy_password=

[AWS]
region=us-east-1


[WebRequest-Test]
metrics_category_name=Web/Request/Test
index=logstash-*
doc_type=api_logs
query_type=aggregate
query={"fields":["timestamp","response","client_ip"],"query": {"filtered": { "query": [ {"match":{"response":200}},{"match":{"environment":"test"}},{"match":{"_type":"nginx"}} ],"filter": {"bool":{"must":[{"range":{"received_at":{"gte":"now-5m"}}}]}}}}}

[WebRequest-Prod]
metrics_category_name=Web/Request/Prod
index=logstash-*
doc_type=nginx
query_type=aggregate
query={"fields":["timestamp","response","client_ip"],"query": {"filtered": { "query": [ {"match":{"response":200}},{"match":{"environment":"test"}},{"match":{"_type":"nginx"}} ],"filter": {"bool":{"must":[{"range":{"received_at":{"gte":"now-5m"}}}]}}}}}
