<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config>
  <datasets>
    <dataset id="results" title="Results" location="${data_root}/*/*.nc" queryable="true" downloadable="false" dataReaderClass="" copyrightStatement="" moreInfo="" disabled="false" updateInterval="${updateInterval}">
    </dataset>
  </datasets>
  <cache enabled="${cache}">
    <inMemorySizeMB>256</inMemorySizeMB>
    <elementLifetimeMinutes>10.0</elementLifetimeMinutes>
  </cache>
  <dynamicServices>
    <dynamicService alias="outputs" servicePath="${data_root}" datasetIdMatch=".*nc" dataReaderClass="" copyrightStatement="" moreInfoUrl="" disabled="false" queryable="true" downloadable="false"/>
  </dynamicServices>
  <contact>
    <name>${contact}</name>
    <organization>${organization}</organization>
    <telephone></telephone>
    <email>${email}</email>
  </contact>
  <server>
    <title>${title}</title>
    <allowFeatureInfo>true</allowFeatureInfo>
    <maxImageWidth>1024</maxImageWidth>
    <maxImageHeight>1024</maxImageHeight>
    <abstract>${abstract}</abstract>
    <keywords>${keywords}</keywords>
    <url>${url}</url>
    <allowglobalcapabilities>${allowglobalcapabilities}</allowglobalcapabilities>
  </server>
</config>
