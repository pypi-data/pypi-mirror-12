#!/bin/sh
ps ax -o user,%cpu,time,command|grep /opt/g|grep -v grep|cut -d/ -f1
ps ax -o user,command|grep /opt/g|grep -v grep|wc -l|awk '{print $1}'
s=`ps ax -o %cpu|awk '{tmp=sprintf("%.0f",($1+20)/100);sum=sum + tmp}END{print sum}'`
printf "%.0f in " $s
echo `cat /proc/cpuinfo |grep processor|wc -l`
#s=`ps ax -o %cpu|awk '{sum=sum + $1}END{print sum/100}'`
#printf "%.0f\n" $s
#printf "%.0f\n" `top n 1 b |head -20|awk 'NR>=8{sum=sum + $9}END{print sum/100}'`
