            '),'#39'S'#39'))'
          #9'UNION'
          #9#9'SELECT'
          #9#9#9'CLIENTE_ID,'
          #9#9#9'NOMBRE_CLIENTE,'
          #9#9#9'DOCTO_CC_ID,'
          #9#9#9'FECHA,'
          #9#9#9'NOMBRE_CONCEP,'
          #9#9#9'FOLIO,'
          #9#9#9'IMPORTE_CARGO,'
          #9#9#9'IMPORTE_CREDITO,'
          #9#9#9'SALDO_FIN,'
          #9#9#9'MOVTO_ESPECIAL,'
          #9#9#9#39'DOLARES'#39' AS MONEDA'
          
            #9#9'FROM (SELECT * FROM ORSP_CL_EDOSCTA_EX(:F_FECHA_INICIAL,:F_FEC' +
            'HA_FINAL, (SELECT CLIENTE_ID FROM CLAVES_CLIENTES WHERE CLAVE_CL' +
            'IENTE= :CLAVE_MN||'#39'D'#39'), '#39'S'#39'))),'
          #9#9'(SELECT '
          #9#9#9'CARGOS_CXC AS CARGOS_PESOS,'
          #9#9#9'CREDITOS_CXC AS CREDITOS_PESOS,'
          #9#9#9'SALDO_FIN_CXC AS TOTAL_PESOS'
          
            #9#9'FROM ORSP_CL_AUX_CLI((SELECT CLIENTE_ID FROM CLAVES_CLIENTES W' +
