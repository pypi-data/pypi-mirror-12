          #9#9#9'SALDO_FIN_CXC AS TOTAL_DOLARES'
          
            #9#9'FROM ORSP_CL_AUX_CLI((SELECT CLIENTE_ID FROM CLAVES_CLIENTES W' +
            'HERE CLAVE_CLIENTE = :CLAVE_MN||'#39'D'#39'),:F_FECHA_INICIAL,:F_FECHA_F' +
            'INAL,'#39'N'#39','#39'N'#39')),'
