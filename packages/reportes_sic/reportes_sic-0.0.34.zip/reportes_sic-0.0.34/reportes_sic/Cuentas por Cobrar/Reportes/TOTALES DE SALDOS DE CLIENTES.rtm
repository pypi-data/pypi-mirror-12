        LinkColor = clBlack
        MaxSQLFieldAliasLength = 0
        SQLText.Strings = (
          'SELECT * FROM ('
          #9'SELECT * FROM ('
          #9#9'SELECT '
          #9#9#9'CLIENTE_ID,'
          #9#9#9'NOMBRE_CLIENTE,'
          #9#9#9'DOCTO_CC_ID,'
          #9#9#9'FECHA,'
          #9#9#9'NOMBRE_CONCEP,'
          #9#9#9'FOLIO,'
          #9#9#9'IMPORTE_CARGO,'
          #9#9#9'IMPORTE_CREDITO,'
          #9#9#9'SALDO_FIN,'
          #9#9#9'MOVTO_ESPECIAL,'
          #9#9#9#39'PESOS'#39' AS MONEDA'
          
            #9#9'FROM ORSP_CL_EDOSCTA_EX(:F_FECHA_INICIAL,:F_FECHA_FINAL, (SELE' +
