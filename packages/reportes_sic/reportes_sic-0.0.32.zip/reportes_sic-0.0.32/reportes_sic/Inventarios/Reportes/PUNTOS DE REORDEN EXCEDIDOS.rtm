        SQLText.Strings = (
          'SELECT ('
          
            #9'SELECT CLAVE_ARTICULO FROM GET_CLAVE_ART(A.ARTICULO_ID)) AS CLA' +
            'VE_ARTICULO,'
          
            #9'(SELECT AL.ALMACEN_ID FROM ALMACENES AL WHERE AL.NOMBRE=:ALMACE' +
