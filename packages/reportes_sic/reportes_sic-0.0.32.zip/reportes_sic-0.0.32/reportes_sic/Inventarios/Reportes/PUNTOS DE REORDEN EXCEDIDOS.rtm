          #9'A.NOMBRE,'
          #9'EST.UNIDAD_MEDIDA,'
          #9'EST.EXISTENCIA,'
          #9'EST.UNID_POR_PEDIR AS EXCEDENTE,'
          
            #9'(SELECT (VALOR_TOTAL/EXISTENCIA) FROM GET_CAPAS_COSTEO(A.ARTICU' +
            'LO_ID,ALMACEN_ID,'#39'C'#39','#39'N'#39')) AS PROM,'
          
            #9'(SELECT (VALOR_TOTAL/EXISTENCIA) FROM GET_CAPAS_COSTEO(A.ARTICU' +
            'LO_ID,ALMACEN_ID,'#39'C'#39','#39'N'#39'))* EST.UNID_POR_PEDIR AS TOTAL'
          'FROM ARTICULOS A'
          
            'LEFT JOIN ESTATUS_ALMS_ART(A.ARTICULO_ID,(SELECT AL.ALMACEN_ID F' +
            'ROM ALMACENES AL WHERE AL.NOMBRE=:ALMACEN) ,:F_FECHA,'#39'E'#39','#39'M'#39') ES' +
            'T ON EST.ARTICULO_ID = A.ARTICULO_ID'
